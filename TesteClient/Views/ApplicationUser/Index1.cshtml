@model IEnumerable<TesteClient.Models.ApplicationUser>

<p></p>
<h3>Liste des clients</h3>
<hr />
<a class="btn btn-secondary btn-sm" asp-area="Identity" asp-page="/Account/Register">Créer un compte</a>
<p></p>
<font size="-1">
    <table class="table table-striped table-bordered table table-dark">

        
    <thead>
        <tr>
            <th>Id</th>
                <th>Nom</th>
                <th>Prenom</th>
                <th>Identifiant</th>
                <th>Mot de passe</th>
                <th>Telephone</th>
                <th>Email</th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                 <td>@user.Id</td>
                 <td>@user.LastName</td>
                 <td>@user.FirstName</td>
                 <td>@user.UserName</td>
                 <td>@user.PasswordHash</td>
                 <td>@user.PhoneNumber</td>
                 
                    <td>
                        <a class="nav-link text-black" asp-area="Identity" asp-page="/Account/Register"> @user.Email</a>
                    </td>
                 
             </tr>
        }
    </tbody>
    

    </table>

</font>
